		"@parcel/transformer-typescript-types": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"eslint": "^8.1.0",
		"parcel": "^2.0.0",
		"typescript": "^4.4.4"
	},
	"source": "index.ts",
	"main": "dist/index.js",
